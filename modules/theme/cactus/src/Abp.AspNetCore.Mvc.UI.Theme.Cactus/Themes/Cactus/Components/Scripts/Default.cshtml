@{
    string cdnHost = ViewBag.CdnHost;
}

@if (cdnHost.IsNullOrWhiteSpace())
{
    <abp-script-bundle>
        <abp-script src="/libs/blog/js/jquery/jquery-3.5.1.slim.min.js" />
        <abp-script src="/libs/blog/js/jquery-cookie/jquery.cookie.min.js" />
        <abp-script src="/libs/blog/js/main.js" />
    </abp-script-bundle>
}
else
{
    <script src="@cdnHost/libs/blog/js/jquery/jquery-3.5.1.slim.min.js"></script>
    <script src="@cdnHost/libs/blog/js/jquery-cookie/jquery.cookie.min.js"></script>
    <script src="@cdnHost/libs/blog/js/main.js"></script>
}
