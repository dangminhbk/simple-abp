@page
@using Simple.Abp.Blog.Web.Front.Shared.Components.Project
@using Simple.Abp.Articles.Web.Front.Shared.Components.Pagination
@model Simple.Abp.Blog.Web.Front.Pages.ProjectsModel
@{
    ViewBag.Title = "Projects";
}

<div id="archive">
    <ul class="project-list">
        @(await Component.InvokeAsync<ProjectViewComponent>(new { modelList = Model.Projects }))
    </ul>

    @(await Component.InvokeAsync<PaginationViewComponent>(new { vm = Model.PaginationViewModel }))
</div>


